{
  "name": "react-with-webpack-4",
  "version": "1.0.0",
  "description": "React with webpack 4",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent",
    "build": "webpack --config webpack.prod.js",
    "serve": "webpack-dev-server  --config webpack.dev.js --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohankveer/react-webpack-4"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "copy-webpack-plugin": "^6.0.2",
    "core-js": "^3.2.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "regenerator-runtime": "^0.13.3",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__tests__/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/__tests__/mocks/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/app/__tests__/utils/testConfig.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "collectCoverageFrom": [
      "app/**/*.js?(x)",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ]
  }
}
